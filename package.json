{
  "name": "vapi-render-webhook",
  "version": "1.0.0",
  "description": "Health America Vapi Agent Alex webhook server with robust guardrails and massive conversational flows.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "build-flow": "node build_flow.js"
  },
  "author": "Health America Dev Team",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "googleapis": "^134.0.0",
    "helmet": "^7.1.0",
    "node-fetch": "^2.7.0",
    "stripe": "^16.0.0"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0"
  },
  "engines": {
    "node": ">=18 <25"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "keywords": [
    "health-america",
    "vapi",
    "alex-agent",
    "conversational-ai",
    "render",
    "webhook",
    "zoho",
    "sku-mapping"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wcurington/vapi-mood.git"
  },
  "bugs": {
    "url": "https://github.com/wcurington/vapi-mood/issues"
  },
  "homepage": "https://render.com/"
}